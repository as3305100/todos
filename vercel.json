{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/todos",
        "use": "index.js"
      }, 
      {
        "src": "/todos/(.*)",
        "dest": "/index.js"
      },
      {
       "src": "/",
        "use": "public/index.html" 
      }
    ]
  } 
  